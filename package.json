{
  "name": "template-css-js-hot-reloader",
  "version": "1.0.0",
  "description": "Template build sass/css, react/js with Hot Reloader",
  "scripts": {
    "start": "node server/express-server.js",
    "start:dev": "npm run clean && node server/hot-reload-server.js",
    "dev": "npm run start:dev",
    "build": "npm run clean && cross-env NODE_ENV=production ./node_modules/.bin/webpack --config webpack.config.prod.js",
    "build:dev": "npm run clean && ./node_modules/.bin/webpack --config webpack.config.dev.js",
    "clean": "npm run clean:css && npm run clean:js && npm run clean:font",
    "clean:css": "rimraf ./public/static/js/app.css",
    "clean:js": "rimraf ./public/static/js/app.js && rimraf ./public/static/js/react.js",
    "clean:font": "rimraf ./public/static/fonts/*"
  },
  "author": "CSTD",
  "license": "MIT",
  "devDependencies": {
    "babel-core": "^6.4.5",
    "babel-loader": "^6.2.1",
    "babel-preset-es2015": "^6.3.13",
    "babel-preset-react": "^6.3.13",
    "babel-preset-stage-0": "^6.3.13",
    "babel-preset-react-hmre": "^1.0.1",
    "css-loader": "^0.23.1",
    "cross-env": "^1.0.7",
    "express": "^4.13.3",
    "extract-text-webpack-plugin": "^1.0.1",
    "file-loader": "^0.8.5",
    "node-sass": "^3.4.2",
    "normalize.css": "^3.0.3",
    "sass-loader": "^3.1.2",
    "style-loader": "^0.13.0",
    "webpack": "^1.12.11",
    "webpack-dev-middleware": "^1.5.0",
    "webpack-hot-middleware": "^2.6.0",
    "react": "^0.14.5",
    "react-dom": "^0.14.5",
    "rimraf": "^2.5.0"
  }
}
